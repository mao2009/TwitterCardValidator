{
    "name": "Twitter Card Validator",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Card validatorでカードが現れるまでボタンを押し続ける",
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "content_scripts": [{
      "matches": ["https://cards-dev.twitter.com/validator"],
      "js": [
        "content.js"
      ]
    }]
  }